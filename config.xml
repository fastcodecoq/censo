    id      = "com.apmontelibano.censo"
    version     = "2.2">

    <name>Censo APmontelíbano</name>

    <description>
        Censo usuarios Comerciales Montelíbano
    </description>

    <author href="http://gomosoft.pro"
        email="info@gomosoft.com">
        Gomosöft
    </author>

    <gap:platforms>
        <gap:platform name="android" minVersion="4.0" />
        <gap:platform name="blackberry" project="widgets"/>
    </gap:platforms>

    <icon src="appicon.png" gap:role="default" />
